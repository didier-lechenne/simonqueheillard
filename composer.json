{
  "name": "getkirby/starterkit",
  "description": "Kirby Starterkit",
  "type": "project",
  "keywords": [
    "kirby",
    "cms",
    "starterkit"
  ],
  "authors": [
    {
      "name": "Bastian Allgeier",
      "email": "bastian@getkirby.com",
      "homepage": "https://getkirby.com"
    }
  ],
  "homepage": "https://getkirby.com",
  "support": {
    "email": "support@getkirby.com",
    "issues": "https://github.com/getkirby/starterkit/issues",
    "forum": "https://forum.getkirby.com",
    "source": "https://github.com/getkirby/starterkit"
  },
  "require": {
    "php": "~8.1.0 || ~8.2.0 || ~8.3.0 || ~8.4.0",
    "getkirby/cms": "5.1.2",
    "d4l/kirby-static-site-generator": "^1.13",
    "getkirby/staticache": "^1.0",
    "sylvainjule/embed": "^1.1",
    "nerdcel/kirby3-responsive-images": "^3.1",
    "philippoehrlein/typo-and-paste": "^2.2",
    "jolicode/jolitypo": "1.4.0",
    "sylvainjule/footnotes": "^2.0",
    "moritzebeling/kirby-maintenance": "^1.0",
    "junohamburg/kirby-reload-on-save": "^1.0",
    "grommasdietz/kirby-hidden-characters": "^1.0",
    "hananils/kirby-date-methods": "^2.1",
    "philippoehrlein/kirby-navigation-groups": "^1.1"
    
  },
  "config": {
    "allow-plugins": {
      "getkirby/composer-installer": true
    },
    "optimize-autoloader": true
  },
  "scripts": {
    "start": [
      "Composer\\Config::disableProcessTimeout",
      "@php -S localhost:8000 kirby/router.php"
    ]
  }
}
